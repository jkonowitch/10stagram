<h1>Add a New Handle</h1>


<h1><%= flash[:notice]%></h1>

<form action="<%=user_handles_path(@user)%>" method="POST">
	<input name="authenticity_token" type="hidden" value="<%=form_authenticity_token%>"/>

	<label>
		Instagram Handle:
		<input type="text" name="handle">
	</label>

	<button>Add Handle</button>
</form>

